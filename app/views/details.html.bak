 <style>
 	.red{
    color: red;
}

.blue{
    color: blue;   
}

.green{
    color: green;
}

 	</style>
 	
 	<meta http-equiv="X-UA-Compatible" content="IE=edge">
   <meta name="viewport" content="width=device-width, initial-scale=1">
   <link rel="stylesheet" href="menu/styles.css">
   <script src="http://code.jquery.com/jquery-latest.min.js" type="text/javascript"></script>
   <script src="menu/script.js"></script>
</head>
<body>



<div class="col-md-12" style="border : none;">
	<div class="col-md-3">
		<div class="cssmenu" id='cssmenu'>
	    <ul>
	       <li class='active has-sub'><a href='#'>Year</a>
	       		<ul>
	             <li ><a href='#'>2016</a></li>
	             <li ><a href='#' style="color:red">2015</a></li>
	             <li ><a href='#'>2014</a></li>
	          </ul>
	       </li>   
	       <li class='active has-sub'><a href='#'>Type</a>
	          <ul>
	             <li ><a href='#'>By Year</a></li>
	             <li ><a href='#' style="color:red">By Month</a></li>
	             <li ><a href='#'>Weekly Avg By Month</a></li>
	             <li ><a href='#'>By Week</a></li>
	          </ul>
	       </li>
	       	 
	    </ul>
	    </div>	
	</div>
	<div class="col-md-9">
		<form class="form-inline">
        <div class="form-group" style="float:right; margin-bottom:25px">
            <label >  </label>
            <input type="text" ng-model="search" class="form-control" placeholder="Search">
        </div>
    </form>
		<div>
				<div class="datatable" id="datatable" >
					<p style="color:white;float:center"><em> For Month JAN</em></p>
<table class="table  table-condensed">
           <!-- <thead>
            <tr>
                <th ng-repeat="header in headers" width="{{header.width}}">{{header.label}}</th>
            </tr>
            </thead>  -->
             <thead>
           <tr>
            <th ng-click="sort('Region')" style="color:white">Region
                <span class="glyphicon sort-icon" ng-show="sortKey=='region'" ng-class="{'glyphicon-chevron-up':reverse,'glyphicon-chevron-down':!reverse}"></span>
            </th>
            <th ng-click="sort('Sold')" style="color:white">Sold $
                <span class="glyphicon sort-icon" ng-show="sortKey=='Sold'" ng-class="{'glyphicon-chevron-up':reverse,'glyphicon-chevron-down':!reverse}"></span>
            </th>
            <th ng-click="sort('Objective')" style="color:white">Objective $
                <span class="glyphicon sort-icon" ng-show="sortKey=='Objective'" ng-class="{'glyphicon-chevron-up':reverse,'glyphicon-chevron-down':!reverse}"></span>
            </th>
            <th ng-click="sort('Diff')" style="color:white">Diff $
                <span class="glyphicon sort-icon" ng-show="sortKey=='Diff'" ng-class="{'glyphicon-chevron-up':reverse,'glyphicon-chevron-down':!reverse}"></span>
            </th>
            <th ng-click="sort('NetSVC')" style="color:white">Net % to SVC Rev
                <span class="glyphicon sort-icon" ng-show="sortKey=='NetSVC'" ng-class="{'glyphicon-chevron-up':reverse,'glyphicon-chevron-down':!reverse}"></span>
            </th>
            <th ng-click="sort('YOYNet')" style="color:white">YOY Net Dollars
                <span class="glyphicon sort-icon" ng-show="sortKey=='YOYNet'" ng-class="{'glyphicon-chevron-up':reverse,'glyphicon-chevron-down':!reverse}"></span>
            </th>
           </tr>
    </thead>
            
            <tbody>
            <tr class="ng-grid" ng-grid="gridOptions" ng-repeat="user in data2|orderBy:sortKey:reverse|filter:search"  style="color:white" ng-dblclick="rowDoubleClicked(user)">
                <td>{{user.region}}</td>
                <td>{{user.Sold}}</td>
                <td>{{user.Objective}}</td>
                <!--glyphicons glyphicons-circle-arrow-top 
                <td ng-class="user.color" >{{user.Diff}}</td>  -->
                <td ng-if="user.Diff >= 0 "><span class="glyphicon glyphicon-arrow-up" style="color:green"></span>{{user.Diff}}</td>
                <td ng-if="user.Diff < 0 "><span class="glyphicon glyphicon-arrow-down" style="color:red"></span>{{user.Diff}}</td> 
                <td ng-if="user.NetSVC >= 0 "><span class="glyphicon glyphicon-arrow-up" style="color:green"></span>{{user.NetSVC}}%</td>
                <td ng-if="user.NetSVC < 0 "><span class="glyphicon glyphicon-arrow-down" style="color:red"></span>{{user.NetSVC}}%</td>  
                <td style="color:green" ng-if="user.YOYNet >= 0 "> {{user.YOYNet}}</td>
                <td style="color:red" ng-if="user.YOYNet < 0 ">{{user.YOYNet}}</td> 
            </tr>
            </tbody>
            <tfoot>

            </tfoot>
        </table>
        
   

</div>
		</div>	
	</div>
</div>

</body>
</html>
